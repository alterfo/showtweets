<h1> Tweets for @<%= name %></h1>

<ul>
	<% tweets.forEach(function(tweet) { %>
		<li>
			<%= tweet.text %><br/>
			<% if (tweet.entities.urls[0] !== undefined) { %>
				<a href="<%= tweet.entities.urls[0].expanded_url %>" target="_blank"><%= tweet.entities.urls[0].display_url %></a>
			<% } %>
		</li>
	<%  }); %>
	
</ul>